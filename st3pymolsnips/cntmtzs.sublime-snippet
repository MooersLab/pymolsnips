print("Count the number of mtz structure factor files in current directory.");
print("Usage: cntmtzs");
myPath = os.getcwd();
mtzCounter = len(glob.glob1(myPath,"*.mtz"));
print("Number of number of mtz structure factor  files in the current directory: ", mtzCounter);
